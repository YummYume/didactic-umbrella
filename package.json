{
  "name": "hackathon-2024",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun --bun run vite dev --host\" \"bun run db:studio\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write . && eslint . --fix",
    "db:push": "drizzle-kit push",
    "db:migrate": "bun --bun run ./drizzle/commands/migrate.ts",
    "db:drop": "bun --bun run ./drizzle/commands/drop.ts",
    "db:create": "bun --bun run ./drizzle/commands/create.ts",
    "db:studio": "bunx drizzle-kit studio --host 0.0.0.0 --verbose --config=./drizzle/drizzle.config.ts"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.1.195",
    "@iconify-json/tabler": "^1.1.114",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.81.0",
    "@sveltejs/adapter-node": "^5.2.0",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/bun": "^1.1.5",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.8",
    "autoprefixer": "^10.4.19",
    "bits-ui": "^0.21.10",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.22.7",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-svelte": "^2.40.0",
    "globals": "^15.6.0",
    "mode-watcher": "^0.3.1",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "sass": "^1.77.6",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.8.1",
    "svelte-sonner": "^0.3.24",
    "sveltekit-flash-message": "^2.4.4",
    "sveltekit-superforms": "^2.15.1",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.0.0-alpha.20",
    "unplugin-icons": "^0.19.0",
    "valibot": "^0.33.3",
    "vite": "^5.3.1"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@mistralai/mistralai": "^0.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.31.2",
    "lucia": "^3.2.0",
    "mysql2": "^3.10.1",
    "oslo": "^1.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss-animate": "^1.0.7"
  }
}
